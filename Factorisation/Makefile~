# partie du makefile a modifier
EXE=Factorize

SRCS=main.cpp RSAKey.cpp
CPPFLAGS=-g -std=c++11 -Wall  -Wextra -lntl -lpthread -pedantic

LDLIBS=-lm

# partie a ne pas toucher
OBJS=$(SRCS:.cpp=.o)
$(EXE): depend $(OBJS)
	$(CXX) $(OBJS) $(LDLIBS) -o $@ -lntl -lpthread
depend:
	makedepend -Y $(SRCS) 2> /dev/null
clean:
	rm -f $(OBJS)
# DO NOT DELETE

main.o: CryptoTools.hpp Decalage.hpp Cryptage.hpp Vigenere.hpp TextBloc.hpp
main.o: RSAKey.hpp
RSAKey.o: RSAKey.hpp

